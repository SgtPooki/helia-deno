{
  "name": "@sgtpooki/helia-deno",
  "description": "An example app created with Deno",
  "type": "module",
  "private": "true",
  "publishConfig": {
    "access": "restricted",
    "registry": "https://noop.com"
  },
  "scripts": {
    "reset": "rimraf node_modules; deno cache --reload main.ts && npm install",
    "dev": "deno run --allow-env --allow-all main.ts",
    "postinstall": "patch-package && run-p cp:*",
    "cp:@achingbrain/ip-address": "cpr 'node_modules/@achingbrain/ip-address' 'node_modules/.deno/@libp2p+utils@3.0.13/node_modules/@achingbrain/ip-address' -o"
  },
  "dependencies": {
    "helia": "1.3.9"
  },
  "devDependencies": {
    "cpr": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "patch-package": "^8.0.0",
    "rimraf": "^5.0.1"
  }
}
